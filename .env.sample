##### GEMINI LLM ######
GEMINI_API_KEY="your-gemini-api-key"
GEMINI_MODEL="gemini-3.0-flash"

##### PROMPT ######
PROMPT_VERSION=3  # 1=roast-master, 2=polite-analyst, 3=dry-humor-analyst

##### LANCEDB / EMBEDDINGS ######
LANCEDB_URI=".lancedb"
DESTINATION__LANCEDB__EMBEDDING_MODEL_PROVIDER="sentence-transformers"
DESTINATION__LANCEDB__EMBEDDING_MODEL="all-MiniLM-L6-v2"
DESTINATION__LANCEDB__CREDENTIALS__URI=".lancedb"

##### RAG ######
RAG_TOP_K=10
CHUNK_SIZE=2000
CHUNK_OVERLAP=100

##### PHOENIX TRACING ######
PHOENIX_COLLECTOR_ENDPOINT="http://localhost:6006/v1/traces"
PHOENIX_PROJECT_NAME="diveroast"
